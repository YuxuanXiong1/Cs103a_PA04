<h1>Transaction Summary By Category</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Category</th>
            <th>total</th>
        </tr>
    </thead>
    <tbody>
        <% results.map(item => {
            if (item && item._id && item._id.category) { %>
            <tr>
               <td> <%= item._id.category %> </td>
               <td><%= item.total %></td>
            </tr>
            <% } %>
        <% }) %>
    </tbody>
</table>

<h1>Transaction Summary By Category</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Category</th>
            <th>total</th>
        </tr>
    </thead>
    <tbody>
        <% results.map(item => {
             %>
            <tr>
               <td> <%= item._id %> </td>
               <td><%= item.totalShuLiang %></td>
            </tr>
            
        <% }) %>
    </tbody>
</table>
